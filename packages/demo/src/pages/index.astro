---
const example = await fetch('https://example.com/').then(res => res.text());
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width" />
		<title>Astro</title>
	</head>
	<body>
		<div class="markup">
			<h1>Markup</h1>
			<Fragment set:html={example} />
		</div>
	</body>
</html>
